<?xml version="1.0"?>
<!DOCTYPE Xdmf SYSTEM "Xdmf.dtd" []>
<Xdmf Version="3.0" xmlns:xi="https://www.w3.org/2001/XInclude">
  <Domain>
    <Grid Name="mesh" GridType="Uniform">
      <Topology TopologyType="Hexahedron" NumberOfElements="256" NodesPerElement="8">
        <DataItem Dimensions="256 8" NumberType="Int" Format="HDF">omega_finest_dg0.h5:/Mesh/mesh/topology</DataItem>
      </Topology>
      <Geometry GeometryType="XYZ">
        <DataItem Dimensions="578 3" Format="HDF">omega_finest_dg0.h5:/Mesh/mesh/geometry</DataItem>
      </Geometry>
    </Grid>
    <Grid Name="p_m" GridType="Collection" CollectionType="Temporal">
      <Grid Name="p_m" GridType="Uniform">
        <xi:include xpointer="xpointer(/Xdmf/Domain/Grid[@GridType='Uniform'][1]/*[self::Topology or self::Geometry])" />
        <Time Value="0" />
        <Attribute Name="p_m" AttributeType="Scalar" Center="Cell">
          <DataItem Dimensions="256 1" Format="HDF">omega_finest_dg0.h5:/Function/p_m/0</DataItem>
        </Attribute>
      </Grid>
    </Grid>
  </Domain>
</Xdmf>
